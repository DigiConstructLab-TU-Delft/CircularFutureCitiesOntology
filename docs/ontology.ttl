@prefix : <https://w3id.org/cfc#CompleteOntology> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/cfc#CompleteOntology> .

<https://w3id.org/cfc#CompleteOntology> rdf:type owl:Ontology ;
                                         owl:imports <http://ld-ce.com/vocab/CAMO> ,
                                                     <http://ld-ce.com/vocab/CEO> ,
                                                     <http://w3id.org/omg> ,
                                                     <https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl> ,
                                                     <https://w3c-lbd-cg.github.io/opm> ,
                                                     <https://w3id.org/bot> ,
                                                     <https://w3id.org/bpo> ,
                                                     <https://w3id.org/digitalconstruction/Agents> ,
                                                     <https://w3id.org/digitalconstruction/Lifecycle> ,
                                                     <https://w3id.org/digitalconstruction/Materials> ;
                                         rdfs:comment "An OWL ontology integrating city-scale material passports, building topology, material types, lifecycle agents, and circular economy concepts (CEO & CAMO), explicitly aligned with OntoCityGML."@en ;
                                         rdfs:label "CFC Ontology - Complete Circular Future Cities Material Passport"@en ;
                                         owl:versionInfo "1.2" .

#################################################################
#    Object Properties
#################################################################

###  http://ld-ce.com/vocab/CAMO#orders
<http://ld-ce.com/vocab/CAMO#orders> rdf:type owl:ObjectProperty .


###  http://ld-ce.com/vocab/CAMO#responsibleForAsset
<http://ld-ce.com/vocab/CAMO#responsibleForAsset> rdf:type owl:ObjectProperty .


###  http://ld-ce.com/vocab/CEO#involvesAgent
<http://ld-ce.com/vocab/CEO#involvesAgent> rdf:type owl:ObjectProperty .


###  http://ld-ce.com/vocab/CEO#offeredBy
<http://ld-ce.com/vocab/CEO#offeredBy> rdf:type owl:ObjectProperty .


###  http://ld-ce.com/vocab/CEO#requestedBy
<http://ld-ce.com/vocab/CEO#requestedBy> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#generated
<http://www.w3.org/ns/prov#generated> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasAssociatedWith
<http://www.w3.org/ns/prov#wasAssociatedWith> rdf:type owl:ObjectProperty .


###  https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject
<https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject> rdf:type owl:ObjectProperty .


###  https://w3id.org/cfc#assignedToAsset
<https://w3id.org/cfc#assignedToAsset> rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://ld-ce.com/vocab/CAMO#responsibleForAsset> ;
                                       rdfs:domain <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                       rdfs:range <https://w3id.org/cfc#Asset> ;
                                       rdfs:comment "Indicates that an Agent (e.g., inspector, maintenance crew) is responsible for a specific Asset."@en ;
                                       rdfs:label "assignedToAsset"@en .


###  https://w3id.org/cfc#derivedFromComponent
<https://w3id.org/cfc#derivedFromComponent> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasDerivedFrom> ;
                                            rdfs:domain <https://w3id.org/cfc#MaterialResource> ;
                                            rdfs:range <https://w3id.org/cfc#BuildingComponent> ;
                                            rdfs:comment "Links a recovered MaterialResource to the original building component it came from."@en ;
                                            rdfs:label "derivedFromComponent"@en .


###  https://w3id.org/cfc#generatesResource
<https://w3id.org/cfc#generatesResource> rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf <http://www.w3.org/ns/prov#generated> ;
                                         rdfs:domain <https://w3id.org/cfc#DemolitionEvent> ;
                                         rdfs:range <https://w3id.org/cfc#MaterialResource> ;
                                         rdfs:comment "Indicates that a demolition event generates a certain material resource for reuse."@en ;
                                         rdfs:label "generatesResource"@en .


###  https://w3id.org/cfc#hasBuilding
<https://w3id.org/cfc#hasBuilding> rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf <https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject> ;
                                   rdfs:domain <https://w3id.org/cfc#Sector> ;
                                   rdfs:range <https://w3id.org/bot#Building> ;
                                   rdfs:comment "Relates a Sector to a Building located within it."@en ;
                                   rdfs:label "hasBuilding"@en .


###  https://w3id.org/cfc#hasDemolitionEvent
<https://w3id.org/cfc#hasDemolitionEvent> rdf:type owl:ObjectProperty ;
                                          rdfs:domain <https://w3id.org/bot#Building> ;
                                          rdfs:range <https://w3id.org/cfc#DemolitionEvent> ;
                                          rdfs:comment "Associates a Building with its planned or actual Demolition event."@en ;
                                          rdfs:label "hasDemolitionEvent"@en .


###  https://w3id.org/cfc#hasMaterial
<https://w3id.org/cfc#hasMaterial> rdf:type owl:ObjectProperty ;
                                   rdfs:domain <https://w3id.org/cfc#BuildingComponent> ;
                                   rdfs:range <https://w3id.org/cfc#Material> ;
                                   rdfs:comment "Indicates a material that a building component is made of."@en ;
                                   rdfs:label "hasMaterial"@en .


###  https://w3id.org/cfc#hasSector
<https://w3id.org/cfc#hasSector> rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf <https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject> ;
                                 rdfs:domain <https://w3id.org/cfc#City> ;
                                 rdfs:range <https://w3id.org/cfc#Sector> ;
                                 rdfs:comment "Relates a City to a Sector (district within the city)."@en ;
                                 rdfs:label "hasSector"@en .


###  https://w3id.org/cfc#makesOffer
<https://w3id.org/cfc#makesOffer> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf <http://ld-ce.com/vocab/CEO#offeredBy> ;
                                  rdfs:domain <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                  rdfs:range <https://w3id.org/cfc#MaterialOffer> ;
                                  rdfs:comment "Links an Agent to a MaterialOffer they create."@en ;
                                  rdfs:label "makesOffer"@en .


###  https://w3id.org/cfc#makesRequest
<https://w3id.org/cfc#makesRequest> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf <http://ld-ce.com/vocab/CEO#requestedBy> ;
                                    rdfs:domain <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                    rdfs:range <https://w3id.org/cfc#MaterialRequest> ;
                                    rdfs:comment "Links an Agent to a MaterialRequest they create."@en ;
                                    rdfs:label "makesRequest"@en .


###  https://w3id.org/cfc#offeredBy
<https://w3id.org/cfc#offeredBy> rdf:type owl:ObjectProperty ;
                                 rdfs:domain <https://w3id.org/cfc#MaterialResource> ;
                                 rdfs:range <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                 rdfs:comment "The agent offering this material resource for reuse."@en ;
                                 rdfs:label "offeredBy"@en .


###  https://w3id.org/cfc#ordersWork
<https://w3id.org/cfc#ordersWork> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf <http://ld-ce.com/vocab/CAMO#orders> ;
                                  rdfs:domain <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                  rdfs:range <https://w3id.org/cfc#WorkOrder> ;
                                  rdfs:comment "Indicates that an Agent issues a WorkOrder for an Asset (e.g., scheduling maintenance, demolition, or reclamation)."@en ;
                                  rdfs:label "ordersWork"@en .


###  https://w3id.org/cfc#participatesInTransaction
<https://w3id.org/cfc#participatesInTransaction> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <http://ld-ce.com/vocab/CEO#involvesAgent> ;
                                                 rdfs:domain <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                                 rdfs:range <https://w3id.org/cfc#MaterialTransaction> ;
                                                 rdfs:comment "Associates an Agent with a completed MaterialTransaction."@en ;
                                                 rdfs:label "participatesInTransaction"@en .


###  https://w3id.org/cfc#performedBy
<https://w3id.org/cfc#performedBy> rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAssociatedWith> ;
                                   rdfs:domain <https://w3id.org/digitalconstruction/Processes#Activity> ;
                                   rdfs:range <https://w3id.org/digitalconstruction/Agents#Agent> ;
                                   rdfs:comment "Indicates the Agent (person or organization) that performs an activity (e.g., demolition, construction, renovation, maintenance, design)."@en ;
                                   rdfs:label "performedBy"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/cfc#hasLoadCapacity
<https://w3id.org/cfc#hasLoadCapacity> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain <https://w3id.org/cfc#BuildingComponent> ;
                                       rdfs:range xsd:decimal ;
                                       rdfs:comment "Specifies the load-bearing capacity (e.g., in kN or MPa) of a structural component or material."@en ;
                                       rdfs:label "hasLoadCapacity"@en .


###  https://w3id.org/cfc#quantity
<https://w3id.org/cfc#quantity> rdf:type owl:DatatypeProperty ;
                                rdfs:domain <https://w3id.org/cfc#MaterialResource> ;
                                rdfs:range xsd:decimal ;
                                rdfs:comment "Quantity of material available (in a specified unit, e.g., kg or count)."@en ;
                                rdfs:label "quantity"@en .


###  https://w3id.org/cfc#unit
<https://w3id.org/cfc#unit> rdf:type owl:DatatypeProperty ;
                            rdfs:domain <https://w3id.org/cfc#MaterialResource> ;
                            rdfs:range xsd:string ;
                            rdfs:comment "Unit of the quantity (e.g., 'kg', 'm^2')."@en ;
                            rdfs:label "unit"@en .



<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/cfc#CompleteOntology#"
     xml:base="https://w3id.org/cfc#CompleteOntology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:ns2="http://www.w3.org/2006/vcard/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/"
     xmlns:schema1="http://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/cfc#CompleteOntology">
        <owl:imports rdf:resource="http://ld-ce.com/vocab/CAMO"/>
        <owl:imports rdf:resource="http://ld-ce.com/vocab/CEO"/>
        <owl:imports rdf:resource="http://w3id.org/omg"/>
        <owl:imports rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl"/>
        <owl:imports rdf:resource="https://w3c-lbd-cg.github.io/opm"/>
        <owl:imports rdf:resource="https://w3id.org/bot"/>
        <owl:imports rdf:resource="https://w3id.org/bpo"/>
        <owl:imports rdf:resource="https://w3id.org/digitalconstruction/Agents"/>
        <owl:imports rdf:resource="https://w3id.org/digitalconstruction/Lifecycle"/>
        <owl:imports rdf:resource="https://w3id.org/digitalconstruction/Materials"/>
        <rdfs:comment xml:lang="en">An OWL ontology integrating city-scale material passports, building topology, material types, lifecycle agents, and circular economy concepts (CEO &amp; CAMO), explicitly aligned with OntoCityGML.</rdfs:comment>
        <rdfs:label xml:lang="en">CFC Ontology - Complete Circular Future Cities Material Passport</rdfs:label>
        <owl:versionInfo>1.2</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://ld-ce.com/vocab/CAMO#orders -->


    <owl:ObjectProperty rdf:about="http://ld-ce.com/vocab/CAMO#orders"/>
    


    <!-- http://ld-ce.com/vocab/CAMO#responsibleForAsset -->


    <owl:ObjectProperty rdf:about="http://ld-ce.com/vocab/CAMO#responsibleForAsset"/>
    


    <!-- http://ld-ce.com/vocab/CEO#involvesAgent -->


    <owl:ObjectProperty rdf:about="http://ld-ce.com/vocab/CEO#involvesAgent"/>
    


    <!-- http://ld-ce.com/vocab/CEO#offeredBy -->


    <owl:ObjectProperty rdf:about="http://ld-ce.com/vocab/CEO#offeredBy"/>
    


    <!-- http://ld-ce.com/vocab/CEO#requestedBy -->


    <owl:ObjectProperty rdf:about="http://ld-ce.com/vocab/CEO#requestedBy"/>
    


    <!-- http://www.w3.org/ns/prov#generated -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#generated"/>
    


    <!-- http://www.w3.org/ns/prov#wasAssociatedWith -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasAssociatedWith"/>
    


    <!-- https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject -->


    <owl:ObjectProperty rdf:about="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject"/>
    


    <!-- https://w3id.org/cfc#assignedToAsset -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#assignedToAsset">
        <rdfs:subPropertyOf rdf:resource="http://ld-ce.com/vocab/CAMO#responsibleForAsset"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#Asset"/>
        <rdfs:comment xml:lang="en">Indicates that an Agent (e.g., inspector, maintenance crew) is responsible for a specific Asset.</rdfs:comment>
        <rdfs:label xml:lang="en">assignedToAsset</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#derivedFromComponent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#derivedFromComponent">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:domain rdf:resource="https://w3id.org/cfc#MaterialResource"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:comment xml:lang="en">Links a recovered MaterialResource to the original building component it came from.</rdfs:comment>
        <rdfs:label xml:lang="en">derivedFromComponent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#generatesResource -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#generatesResource">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#generated"/>
        <rdfs:domain rdf:resource="https://w3id.org/cfc#DemolitionEvent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#MaterialResource"/>
        <rdfs:comment xml:lang="en">Indicates that a demolition event generates a certain material resource for reuse.</rdfs:comment>
        <rdfs:label xml:lang="en">generatesResource</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#hasBuilding -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#hasBuilding">
        <rdfs:subPropertyOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject"/>
        <rdfs:domain rdf:resource="https://w3id.org/cfc#Sector"/>
        <rdfs:range rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:comment xml:lang="en">Relates a Sector to a Building located within it.</rdfs:comment>
        <rdfs:label xml:lang="en">hasBuilding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#hasDemolitionEvent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#hasDemolitionEvent">
        <rdfs:domain rdf:resource="https://w3id.org/bot#Building"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#DemolitionEvent"/>
        <rdfs:comment xml:lang="en">Associates a Building with its planned or actual Demolition event.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDemolitionEvent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#hasMaterial -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#hasMaterial">
        <rdfs:domain rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#Material"/>
        <rdfs:comment xml:lang="en">Indicates a material that a building component is made of.</rdfs:comment>
        <rdfs:label xml:lang="en">hasMaterial</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#hasSector -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#hasSector">
        <rdfs:subPropertyOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#containsCityObject"/>
        <rdfs:domain rdf:resource="https://w3id.org/cfc#City"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#Sector"/>
        <rdfs:comment xml:lang="en">Relates a City to a Sector (district within the city).</rdfs:comment>
        <rdfs:label xml:lang="en">hasSector</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#makesOffer -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#makesOffer">
        <rdfs:subPropertyOf rdf:resource="http://ld-ce.com/vocab/CEO#offeredBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#MaterialOffer"/>
        <rdfs:comment xml:lang="en">Links an Agent to a MaterialOffer they create.</rdfs:comment>
        <rdfs:label xml:lang="en">makesOffer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#makesRequest -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#makesRequest">
        <rdfs:subPropertyOf rdf:resource="http://ld-ce.com/vocab/CEO#requestedBy"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#MaterialRequest"/>
        <rdfs:comment xml:lang="en">Links an Agent to a MaterialRequest they create.</rdfs:comment>
        <rdfs:label xml:lang="en">makesRequest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#offeredBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#offeredBy">
        <rdfs:domain rdf:resource="https://w3id.org/cfc#MaterialResource"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:comment xml:lang="en">The agent offering this material resource for reuse.</rdfs:comment>
        <rdfs:label xml:lang="en">offeredBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#ordersWork -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#ordersWork">
        <rdfs:subPropertyOf rdf:resource="http://ld-ce.com/vocab/CAMO#orders"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#WorkOrder"/>
        <rdfs:comment xml:lang="en">Indicates that an Agent issues a WorkOrder for an Asset (e.g., scheduling maintenance, demolition, or reclamation).</rdfs:comment>
        <rdfs:label xml:lang="en">ordersWork</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#participatesInTransaction -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#participatesInTransaction">
        <rdfs:subPropertyOf rdf:resource="http://ld-ce.com/vocab/CEO#involvesAgent"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/cfc#MaterialTransaction"/>
        <rdfs:comment xml:lang="en">Associates an Agent with a completed MaterialTransaction.</rdfs:comment>
        <rdfs:label xml:lang="en">participatesInTransaction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/cfc#performedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/cfc#performedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasAssociatedWith"/>
        <rdfs:domain rdf:resource="https://w3id.org/digitalconstruction/Processes#Activity"/>
        <rdfs:range rdf:resource="https://w3id.org/digitalconstruction/Agents#Agent"/>
        <rdfs:comment xml:lang="en">Indicates the Agent (person or organization) that performs an activity (e.g., demolition, construction, renovation, maintenance, design).</rdfs:comment>
        <rdfs:label xml:lang="en">performedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/cfc#hasLoadCapacity -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cfc#hasLoadCapacity">
        <rdfs:domain rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Specifies the load-bearing capacity (e.g., in kN or MPa) of a structural component or material.</rdfs:comment>
        <rdfs:label xml:lang="en">hasLoadCapacity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cfc#quantity -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cfc#quantity">
        <rdfs:domain rdf:resource="https://w3id.org/cfc#MaterialResource"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Quantity of material available (in a specified unit, e.g., kg or count).</rdfs:comment>
        <rdfs:label xml:lang="en">quantity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/cfc#unit -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/cfc#unit">
        <rdfs:domain rdf:resource="https://w3id.org/cfc#MaterialResource"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Unit of the quantity (e.g., &apos;kg&apos;, &apos;m^2&apos;).</rdfs:comment>
        <rdfs:label xml:lang="en">unit</rdfs:label>
    </owl:DatatypeProperty>
    



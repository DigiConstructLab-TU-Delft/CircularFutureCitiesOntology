    <!-- https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#BuildingPart -->


    <owl:Class rdf:about="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#BuildingPart"/>
    


    <!-- https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#City -->


    <owl:Class rdf:about="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#City"/>
    


    <!-- https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject -->


    <owl:Class rdf:about="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject"/>
    


    <!-- https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#Site -->


    <owl:Class rdf:about="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#Site"/>
    


    <!-- https://w3id.org/bot#Building -->


    <rdf:Description rdf:about="https://w3id.org/bot#Building">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#Building"/>
        <rdfs:comment xml:lang="en">BOT Building aligned as a subclass of OntoCityGML Building.</rdfs:comment>
    </rdf:Description>
    


    <!-- https://w3id.org/bot#Site -->


    <rdf:Description rdf:about="https://w3id.org/bot#Site">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#Site"/>
        <rdfs:comment xml:lang="en">BOT Site aligned as a subclass of OntoCityGML Site.</rdfs:comment>
    </rdf:Description>
    


    <!-- https://w3id.org/bot#Space -->


    <rdf:Description rdf:about="https://w3id.org/bot#Space">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#BuildingPart"/>
        <rdfs:comment xml:lang="en">BOT Space aligned as a subclass of OntoCityGML BuildingPart.</rdfs:comment>
    </rdf:Description>
    


    <!-- https://w3id.org/bot#Storey -->


    <rdf:Description rdf:about="https://w3id.org/bot#Storey">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#BuildingPart"/>
        <rdfs:comment xml:lang="en">BOT Storey aligned as a subclass of OntoCityGML BuildingPart.</rdfs:comment>
    </rdf:Description>
    


    <!-- https://w3id.org/bpo#assembly -->


    <owl:Class rdf:about="https://w3id.org/bpo#assembly"/>
    


    <!-- https://w3id.org/cfc#AluminumMaterial -->


    <owl:Class rdf:about="https://w3id.org/cfc#AluminumMaterial">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#Material"/>
        <rdfs:comment xml:lang="en">Aluminum material commonly used for window frames, façade cladding, and structural elements; valued for its light weight and recyclability.</rdfs:comment>
        <rdfs:label xml:lang="en">AluminumMaterial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#ArchitectRole -->


    <owl:Class rdf:about="https://w3id.org/cfc#ArchitectRole">
        <rdfs:subClassOf rdf:resource="https://w3id.org/digitalconstruction/Agents#StakeholderRole"/>
        <rdfs:comment xml:lang="en">Role of an agent responsible for architectural design of buildings.</rdfs:comment>
        <rdfs:label xml:lang="en">ArchitectRole</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#Asset -->


    <owl:Class rdf:about="https://w3id.org/cfc#Asset">
        <rdfs:subClassOf rdf:resource="http://ld-ce.com/vocab/CAMO#Asset"/>
        <rdfs:comment xml:lang="en">A tangible building asset or component tracked over its lifecycle.</rdfs:comment>
        <rdfs:label xml:lang="en">Asset</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#AssetCondition -->


    <owl:Class rdf:about="https://w3id.org/cfc#AssetCondition">
        <rdfs:subClassOf rdf:resource="http://ld-ce.com/vocab/CAMO#AssetCondition"/>
        <rdfs:comment xml:lang="en">Represents the condition state of an Asset (e.g., new, in-use, worn, end-of-life).</rdfs:comment>
        <rdfs:label xml:lang="en">AssetCondition</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#Beam -->


    <owl:Class rdf:about="https://w3id.org/cfc#Beam">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:comment xml:lang="en">A horizontal structural member that supports loads.</rdfs:comment>
        <rdfs:label xml:lang="en">Beam</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#BrassMaterial -->


    <owl:Class rdf:about="https://w3id.org/cfc#BrassMaterial">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#Material"/>
        <rdfs:comment xml:lang="en">Brass material used in high-value fittings and fixtures (such as door hardware, taps, and decorative elements); fully recyclable.</rdfs:comment>
        <rdfs:label xml:lang="en">BrassMaterial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#BrickMaterial -->


    <owl:Class rdf:about="https://w3id.org/cfc#BrickMaterial">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#Material"/>
        <rdfs:comment xml:lang="en">Brick material including clay bricks and concrete masonry units, commonly used for walls, façades, and paving.</rdfs:comment>
        <rdfs:label xml:lang="en">BrickMaterial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#BuildingComponent -->


    <owl:Class rdf:about="https://w3id.org/cfc#BuildingComponent">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/bot#Element"/>
        <rdfs:comment xml:lang="en">A physical building element or component that is part of a building (e.g., wall, window, beam).</rdfs:comment>
        <rdfs:label xml:lang="en">BuildingComponent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#City -->


    <owl:Class rdf:about="https://w3id.org/cfc#City">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#City"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/cfc#hasSector"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/cfc#Sector"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A city or municipality containing districts and buildings.</rdfs:comment>
        <rdfs:label xml:lang="en">City</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#CladdingComponent -->


    <owl:Class rdf:about="https://w3id.org/cfc#CladdingComponent">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:comment xml:lang="en">An external cladding component, such as siding, panels, or curtain walls.</rdfs:comment>
        <rdfs:label xml:lang="en">CladdingComponent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#Column -->


    <owl:Class rdf:about="https://w3id.org/cfc#Column">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/TheWorldAvatar/ontology/main/ontology/ontocitygml/ontocitygml.owl#CityObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#BuildingComponent"/>
        <rdfs:comment xml:lang="en">A vertical structural member that supports loads.</rdfs:comment>
        <rdfs:label xml:lang="en">Column</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#ConcreteMaterial -->


    <owl:Class rdf:about="https://w3id.org/cfc#ConcreteMaterial">
        <rdfs:subClassOf rdf:resource="https://w3id.org/cfc#Material"/>
        <rdfs:comment xml:lang="en">Concrete material used in foundations, slabs, walls, and precast elements; typically a mixture of cement, aggregates, and water.</rdfs:comment>
        <rdfs:label xml:lang="en">ConcreteMaterial</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/cfc#ConstructionEvent -->


    <owl:Class rdf:about="https://w3id.org/cfc#ConstructionEvent">
